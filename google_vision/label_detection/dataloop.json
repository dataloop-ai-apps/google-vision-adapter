{
  "displayName": "Google Vision Label Detection",
  "name": "google-vision-label-detection",
  "description": "Label Detection can detect and extract information about entities in an image, across a broad group of categories.  Requires GCS Integration in Dataloop, Refer to Data Governance for more information",
  "scope": "public",
  "version": "0.1.6",
  "attributes": {
    "Provider": "Google",
    "Category": "Application",
    "Deployed By": "Google",
    "Media Type": ["Image"],
    "Application Type": ["Pipeline Node"]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/google-vision-adapter.git",
    "gitTag": "0.1.6"
  },
  "components": {
    "pipelineNodes": [
      {
        "invoke": {
          "type": "function",
          "namespace": "google-vision-label-detection.google_vision_label_detection.label_detection"
        },
        "name": "Label Detection",
        "categories": ["Google Vision"],
        "displayName": "Label Detection",
        "description": "Detects labels for the image",
        "scope": "project",
        "configuration": {
          "fields": [
            {
              "name": "name",
              "title": "Node Name",
              "props": {
                "title": true,
                "type": "string",
                "default": "Label Detection",
                "required": true,
                "placeholder": "Insert node name"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "google_vision_label_detection",
        "entryPoint": "google_vision/label_detection/label_detection.py",
        "className": "ServiceRunner",
        "integrations": [
          "dl-gcp-service-account"
        ],
        "functions": [
          {
            "name": "label_detection",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayIcon": "icon-dl-label-filled"
          }
        ]
      }
    ],
    "services": [
      {
        "name": "google-vision-label-detection",
        "moduleName": "google_vision_label_detection",
        "secrets": [],
        "runtime": {
          "podType": "regular-xs",
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/google_vision:0.4.0",
          "numReplicas": 1,
          "concurrency": 10,
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 10
          },
          "preemptible": false
        },
        "operation": "none"
      }
    ]
  }
}
