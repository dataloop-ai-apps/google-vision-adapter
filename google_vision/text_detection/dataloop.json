{
  "displayName": "Google Vision Text Detection",
  "name": "google-vision-text-detection",
  "scope": "public",
  "version": "0.0.8",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/google-vision-adapter.git",
    "gitTag": "0.0.8"
  },
  "components": {
    "pipelineNodes": [
      {
        "invoke": {
          "type": "function",
          "namespace": "google-vision-text-detection-v2.google_vision_text_detection_v2.text_detection"
        },
        "name": "Text Detection",
        "categories": [
          "Google Vision"
        ],
        "displayName": "Text Detection",
        "description": "Detects text in the image",
        "scope": "public",
        "configuration": {
          "fields": [
            {
              "name": "name",
              "title": "Node Name",
              "props": {
                "title": true,
                "type": "string",
                "default": "Text Detection",
                "required": true,
                "placeholder": "Insert node name"
              },
              "rules": [
                {
                  "type": "required",
                  "effect": "error"
                }
              ],
              "widget": "dl-input"
            }
          ]
        }
      }
    ],
    "modules": [
      {
        "name": "google_vision_text_detection_v2",
        "entryPoint": "google_vision/text_detection/text_detection.py",
        "className": "ServiceRunner",
        "initInputs": [
          {
            "type": "String",
            "name": "integration_name"
          }
        ],
        "functions": [
          {
            "name": "text_detection",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "output": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayIcon": "text"
          }
        ]
      }
    ],
    "services": [
      {
        "name": "google-vision-text-detection-v2",
        "moduleName": "google_vision_text_detection_v2",
        "initParams": {
          "integration_name": "google-vision-ludo"
        },
        "secrets": [],
        "runtime": {
          "podType": "regular-xs",
          "runnerImage": "shadimahameeddl/vision-test:latest",
          "numReplicas": 1,
          "concurrency": 10,
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 1,
            "maxReplicas": 2,
            "queueLength": 10
          },
          "preemptible": false
        },
        "operation": "none"
      }
    ]
  }
}
